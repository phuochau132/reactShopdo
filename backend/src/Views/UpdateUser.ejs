<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet">
</head>

<body>
    <div>

        <%= dataUser%>
            <h1>Hello ae</h1>
            <table>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Action</th>
                </tr>
                <% for(var i=0; i < dataUser.length; i++) { %>
                    <div>
                        <form action="/form-fix-user/<%=dataUser[i].id%>" method="POST">
                            <label for="">Last Name</label>
                            <input type="text" name="firstname" value="<%=dataUser[i].firstname%>">

                            <label for="">First Name</label>
                            <input type="text" name="lastname" value="<%=dataUser[i].lastname%>">

                            <label for="">Email</label>
                            <input type="email" name="email" value="<%=dataUser[i].email%>">

                            <label for="">Address</label>
                            <input type="text" name="address" value="<%=dataUser[i].address%>">

                            <input type="submit" value="OK">
                        </form>
                    </div>
                    <% } %>

            </table>

    </div>

</body>

</html>