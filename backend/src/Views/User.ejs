<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet">
</head>

<body>
    <div>

        <%= dataUser%>
            <h1>Hello ae</h1>
            <table>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Action</th>
                </tr>
                <% for(var i=0; i < dataUser.length; i++) { %>
                    <tr>
                        <td>
                            <%=dataUser[i].id%>
                        </td>
                        <td>
                            <%=dataUser[i].firstname%>
                        </td>
                        <td>
                            <%=dataUser[i].lastname%>
                        </td>
                        <td>
                            <%=dataUser[i].address%>
                        </td>
                        <td>
                            <a href="/user/<%=dataUser[i].id%>">Detail</a>
                            <button>Fix</button>
                            <button>Delete</button>
                        </td>
                    </tr>
                    <% } %>

            </table>

    </div>

</body>

</html>